{
  "author": "Mustafa Oransel",
  "contributors": [
    "Alfred Omega",
    "Vedran Opacic",
    "Kelvin Foo",
    "Chris McGee",
    "Marek Magdziak",
    "hao-wang"
  ],
  "name": "talib",
  "description": "Technical Analysis Library",
  "version": "1.1.6",
  "homepage": "https://github.com/lafafm/node_talib",
  "repository": {
    "type": "git",
    "url": "https://github.com/lafafm/node_talib.git"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "scripts": {
    "build": "node ./src/lib/build.js && node-gyp configure && node-gyp build",
    "build-prebuild": "prebuild --all",
    "install": "node-pre-gyp-github install --fallback-to-build",
    "clean": "node ./src/lib/clean.js"
  },
  "main": "build/Release/talib.node",
  "engines": {
    "node": ">=8.16.0"
  },
  "dependencies": {
    "nan": "^2.13.2",
    "node-pre-gyp-github": "^1.4.3"
  },
  "devDependencies": {
    "prebuild": "^10.0.0"
  },
  "binary": {
    "module_name": "talib",
    "module_path": "./build/Release/",
    "host": "https://github.com/lafafm/node_talib/releases/download/",
    "remote_path": "{version}"
  },
  "optionalDependencies": {},
  "keywords": [
    "talib",
    "stocks",
    "stock market"
  ],
  "licenses": [
    {
      "type": "LGPL License",
      "url": "http://www.gnu.org/licenses/lgpl.html"
    }
  ]
}
